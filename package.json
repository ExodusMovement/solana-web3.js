{
  "name": "@exodus/solana-web3.js",
  "version": "0.87.1-exodus1",
  "description": "Solana Javascript API",
  "keywords": [
    "api",
    "blockchain"
  ],
  "license": "MIT",
  "author": "Solana Maintainers <maintainers@solana.com>",
  "homepage": "https://solana.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ExodusMovement/solana-web3.js.git"
  },
  "bugs": {
    "url": "http://github.com/ExodusMovement/solana-web3.js.git/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "testnetDefaultChannel": "edge",
  "files": [
    "/lib",
    "/module.flow.js"
  ],
  "scripts": {
    "prepare": "babel src --out-dir lib",
    "build:fixtures": "./test/fixtures/noop-c/build.sh; ./test/fixtures/noop-rust/build.sh",
    "clean:fixtures": "examples/bpf-rust-noop/do.sh clean; make -C examples/bpf-c-noop clean ",
    "clean": "rimraf ./coverage ./lib",
    "codecov": "set -ex; npm run test:cover; cat ./coverage/lcov.info | codecov",
    "defs": "set -ex; flow check-contents < module.flow.js; tsc module.d.ts",
    "examples": "set -ex; for example in examples/*.js; do node $example; done",
    "flow": "set -ex; flow stop; flow",
    "flow:stop": "flow stop",
    "flow:watch": "flow stop; watch 'flow' . --wait=1",
    "lint": "set -ex; npm run pretty; eslint . --ext .js,.ts",
    "lint:fix": "npm run pretty:fix && eslint . --fix",
    "lint:watch": "watch 'npm run lint:fix' . --wait=1",
    "localnet:down": "bin/localnet.sh down",
    "localnet:logs": "bin/localnet.sh logs -f",
    "localnet:up": "bin/localnet.sh up",
    "localnet:update": "bin/localnet.sh update",
    "ok": "run-s lint flow test defs",
    "pretty": "prettier --check '{,{examples,src,test}/**/}*.{j,t}s'",
    "pretty:fix": "prettier --write '{,{examples,src,test}/**/}*.{j,t}s'",
    "re": "semantic-release --repository-url git@github.com:ExodusMovement/solana-web3.js.git",
    "test": "cross-env NODE_ENV=test jest --useStderr",
    "test:cover": "npm run build:fixtures && cross-env NODE_ENV=test jest --coverage --useStderr",
    "test:live": "npm run build:fixtures && cross-env NODE_ENV=test TEST_LIVE=1 jest --useStderr",
    "test:watch": "npm run build:fixtures && cross-env NODE_ENV=test jest --watch --useStderr"
  },
  "jest": {
    "testEnvironment": "./jest-environment"
  },
  "dependencies": {
    "bn.js": "^5.0.0",
    "bs58": "^4.0.1",
    "buffer": "^6.0.1",
    "buffer-layout": "^1.2.0",
    "crypto-hash": "^1.2.2",
    "jayson": "^3.0.1",
    "keccak": "^3.0.1",
    "node-fetch": "^2.2.0",
    "rpc-websockets": "^7.4.2",
    "secp256k1": "^4.0.2",
    "superstruct": "^0.8.3",
    "tweetnacl": "^1.0.0",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/travis-cli": "^11.0.0",
    "@solana/spl-token": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "acorn": "^8.0.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "codecov": "^3.0.4",
    "cross-env": "7.0.2",
    "elfy": "^1.0.0",
    "enzyme": "3.11.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "22.19.0",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "0.130.0",
    "flow-typed": "3.2.1",
    "fs-file-tree": "1.1.1",
    "jest": "26.6.3",
    "marked": "^1.1.0",
    "mz": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.0",
    "rimraf": "3.0.2",
    "semantic-release": "^17.0.2",
    "typescript": "^4.0.2",
    "watch": "^1.0.2"
  }
}
